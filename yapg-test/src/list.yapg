extern {
    use crate::list::ast::*;
};

// unique kind for each token
tokenkind Token {
    Num(..) = "number",
    Comma = ",",
    LBracket = "[",
    RBracket = "]",
};

// define SemanticAction
SemanticAction = SemanticAction;

List: List = {
    Token::LBracket Token::RBracket => `make_empty_list`,
    Token::LBracket Item (Token::Comma Item)* Token::RBracket => `append_item`
};

Item: Item = {
    Token::Num => `make_num_item`,
    List => `make_list_item`,
};